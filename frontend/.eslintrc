{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "babel"
  ],
  "rules": {
    "constructor-super": 2,
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "linebreak-style": "off",
    "no-eq-null": 0,
    "one-var": [2, { "initialized": "never" }],
    "prefer-const": 0,
    "no-param-reassign": 0,
    "no-this-before-super": 2,
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
    "babel/object-shorthand": 2,
    "react/require-extension": "off",
    "react/jsx-boolean-value": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/sort-comp": 0,
    "react/prop-types": [2, { "ignore": [
        "children",
        "className",
        "style",
        "bsStyle",
        "bsClass",
        "bsSize"
        ]
    }]
  },
  "globals": {
    "jspData": true
  }
}
